{
  "expo": {
    "name": "Telegram Message Cleaner",
    "slug": "telegram-message-cleaner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0088cc"
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.telecleaner.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0088cc"
      },
      "package": "com.telecleaner.app",
      "enableProguardInReleaseBuilds": true,
      "enableShrinkResourcesInReleaseBuilds": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "telecleaner",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "621a00e6-1dbf-4f57-992e-5f5d1f993271"
      }
    },
    "owner": "snorlaxida"
  }
}
